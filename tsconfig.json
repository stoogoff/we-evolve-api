{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"module": "commonjs",
		"moduleResolution": "Node",
		"noImplicitAny": false,
		"outDir": "dist",
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2022",
		"types": ["node"]
	},
	"exclude": [
		"node_modules/**"
	]
}
